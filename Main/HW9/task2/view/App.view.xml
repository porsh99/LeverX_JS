<mvc:View
    controllerName="sap.ui.demo.db.controller.App"
	xmlns="sap.m"
    xmlns:l="sap.ui.layout"
	xmlns:mvc="sap.ui.core.mvc">
	
   <Panel headerText="{i18n>panelDataHeaderText}" class="sapUiResponsiveMargin" width="auto">
		<content>
			<List
				id="ProductList"
				headerText="{i18n>listHeaderText}"
				items="{
					path: 'products>/Products',
					factory: '.productListFactory'
				}">
					<dependents>
					<ObjectListItem
						id="productExtended"
						title="UserName:{products>FirstName}  LastName:{products>LastName} Gender:{products>Gender} "
						>
					</ObjectListItem>
				</dependents>
			</List>
		</content>
	</Panel>
    
</mvc:View>